class CombatManager {
}
CombatManager *-down- Combat
CombatManager o-right- "0..*" Player

class Combat {
	Combatants allies
	Combatants enemies
	Inventory reward
	Move playerMove
	Move npcMove
	turn()
}
Combat *-right- "2" Move
Combat o-down- "2" Combatants
Combat *-down- "1" Inventory

class Combatants {
}
Combatants o-down- "0..*" Entity

class Move {
	String name
	int blockPercent
	double damage
	double healing
	
}

@enduml
